    "@angular/platform-browser": "2.4.9",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@angular/router": "v3.4.9",
    "angular2-materialize": "6.7.2",
    "angularfire2": "2.0.0-beta.8",
    "core-js": "2.4.1",
    "dragula": "^3.7.2",
    "firebase": "3.7.1",
    "hammerjs": "2.0.8",
    "jquery": "3.1.1",
    "materialize-css": "0.98.0",
    "moment": "2.17.1",
    "ng2-dragula": "^1.2.1",
    "rxjs": "5.2.0",
    "zone.js": "0.8.1"
